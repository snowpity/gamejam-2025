{
    "sequenceId": "example_dialogue",
    "dialogues": [
        {
            "characterName": "Fair Flyer",
            "dialogueText": "Alright, does everypony know the plan?",
            "portraitSpriteRight": "MF_charsheet_0",
            "portraitSpriteLeft": "",
            "emotion": 1,
            "audioCue": ""
        },
        {
            "characterName": "Soirée",
            "dialogueText": "Why are we doing this again?",
            "portraitSpriteRight": "MF_charsheet_3",
            "portraitSpriteLeft": "",
            "emotion": 0,
            "audioCue": ""
        },
        {
            "characterName": "Fair Flyer",
            "dialogueText": "The charity fundraiser is short thousands of bits and the con starts in just a few days! We can't let all those ponies down!",
            "portraitSpriteRight": "MF_charsheet_0",
            "portraitSpriteLeft": "",
            "emotion": 1,
            "audioCue": ""
        },
        {
            "characterName": "Matinée",
            "dialogueText": "So we're opening a last-minute pop-up diner to raise donations the old-fashioned way!",
            "portraitSpriteRight": "MF_charsheet_1",
            "portraitSpriteLeft": "",
            "emotion": 1,
            "audioCue": ""
        },
        {
            "characterName": "Soirée",
            "dialogueText": "Fine. I'll handle the cooking, and Matinée will help me in the kitchen.",
            "portraitSpriteRight": "MF_charsheet_3",
            "portraitSpriteLeft": "",
            "emotion": 0,
            "audioCue": ""
        },
        {
            "characterName": "Fair Flyer",
            "dialogueText": "Great! I'll use WASD to move around and space to interact!",
            "portraitSpriteRight": "MF_charsheet_0",
            "portraitSpriteLeft": "",
            "emotion": 1,
            "audioCue": ""
        },
        {
            "characterName": "Matinée",
            "dialogueText": "Customers will arrive in groups. Select a group and seat them at the tables!",
            "portraitSpriteRight": "MF_charsheet_1",
            "portraitSpriteLeft": "",
            "emotion": 1,
            "audioCue": ""
        },
        {
            "characterName": "Soirée",
            "dialogueText": "They'll read the menu, then raise their hooves when ready to order. Take their order and I'll start cooking.",
            "portraitSpriteRight": "MF_charsheet_3",
            "portraitSpriteLeft": "",
            "emotion": 0,
            "audioCue": ""
        },
        {
            "characterName": "Fair Flyer",
            "dialogueText": "Once the food is ready, I'll pick it up and deliver it to the table.",
            "portraitSpriteRight": "MF_charsheet_0",
            "portraitSpriteLeft": "",
            "emotion": 1,
            "audioCue": ""
        },
        {
            "characterName": "Matinée",
            "dialogueText": "Don't forget to collect payment and dismiss them when they're done!",
            "portraitSpriteRight": "MF_charsheet_1",
            "portraitSpriteLeft": "",
            "emotion": 1,
            "audioCue": ""
        },
        {
            "characterName": "Soirée",
            "dialogueText": "Careful, these fillies can be impatient. If you neglect them, they'll storm off and cost us bits.",
            "portraitSpriteRight": "MF_charsheet_3",
            "portraitSpriteLeft": "",
            "emotion": 0,
            "audioCue": ""
        },
        {
            "characterName": "Matinée",
            "dialogueText": "But if you serve them swiftly, they'll leave a nice tip! Every bit counts for the charity!",
            "portraitSpriteRight": "MF_charsheet_1",
            "portraitSpriteLeft": "",
            "emotion": 1,
            "audioCue": ""
        },

        {
            "characterName": "Morning Mimosa",
            "dialogueText": "Oh! What do I get to do?",
            "portraitSpriteRight": "MF_charsheet_2",
            "portraitSpriteLeft": "",
            "emotion": 1,
            "audioCue": ""
        },

        {
            "characterName": "Fair Flyer",
            "dialogueText": "Uhhh, we'll figure something out!",
            "portraitSpriteRight": "MF_charsheet_0",
            "portraitSpriteLeft": "",
            "emotion": 1,
            "audioCue": ""
        },

        {
            "characterName": "Morning Mimosa",
            "dialogueText": "Wait, I know! I can run the bar! I'll turn it into a full attraction!",
            "portraitSpriteRight": "MF_charsheet_2",
            "portraitSpriteLeft": "",
            "emotion": 1,
            "audioCue": ""
        },

        {
            "characterName": "Soirée",
            "dialogueText": "Did you not tell her we ran out of time to-",
            "portraitSpriteRight": "MF_charsheet_3",
            "portraitSpriteLeft": "",
            "emotion": 0,
            "audioCue": ""
        },

        {
            "characterName": "Fair Flyer",
            "dialogueText": "Anyways! Let's save this charity and run this diner!",
            "portraitSpriteRight": "MF_charsheet_0",
            "portraitSpriteLeft": "",
            "emotion": 1,
            "audioCue": ""
        }
    ]
}